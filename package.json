{
  "name": "hello-world",
  "displayName": "hello_world",
  "description": "hello world",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
        {
          "command": "hello-world.helloWorld",
          "title": "Hello World",
          "category": "Hello",
        },
      {
          "command": "vscodeasy.copytoclipboard0",
          "title": "Copy to clipboard 0",
          "category": "VSCodeasy",
          "icon": {
            "light": "",
            "dark": ""
          },
        
        {
          "command": "hello_world.worldHello",
          "title": "World Hello",
          "category": "Hello"
        }, 

        {
          "command": "hello_world.Open Whiteboard",
          "title": "Open New Whiteboard",
          "category": "Whiteboard"
        },
      {
          "command": "vscodeasy.pastefromclipboard0",
          "title": "Paste from clipboard 0",
          "category": "VSCodeasy",
          "icon": {
            "light": "",
            "dark": ""
          },
        
    ],
    "viewContainers": {
      "activitybar": [
        {
          "id": "hello-world",
          "title": "Hello world"
        }
      ]
    },
    "views": {
      "hello-world": [
        {
          "id": "hello-world1",
          "name": "hello world 1"
        },
        {
          "id": "hello-world2",
          "name": "hello world 2"
        }
      ]
    },
    "keybindings": [
      {
        "command": "vscodeasy.copytoclipboard0",
        "key": "ctrl+alt+C 1",
        "mac": "cmd+alt+C 1",
        "when": "editorTextFocus"
      },
      {
        "command": "vscodeasy.pastefromclipboard0",
        "key": "ctrl+alt+V 1",
        "mac": "cmd+alt+V 1",
        "when": "editorTextFocus"
      },

    ],
  
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  },

  "menus": {
    "commandPallete": [
      {
        "command": "extension.sayHello",
        "when": "editorHasSelection"
      }

    ]
  }
}
